{
    "name": "machour/yii-bootstrap4-ks",
    "type": "project",
    "description": "A kitchen sink for yii-boostrap4",
    "keywords": [
        "yii",
        "bootstrap4",
        "tests"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Mehdi Achour",
            "email": "machour@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii-web": "^3.0@dev",
        "yiisoft/yii-bootstrap4": "^3.0@dev",
        "yiisoft/yii-jquery": "^3.0@dev",
        "yiisoft/yii-console": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/rbac": "^3.0@dev",
        "cebe/markdown": "^1.2@dev"
    },
    "require-dev": {
        "foxy/foxy": "^1.0",
        "yiisoft/yii-debug": "^3.0@dev",
        "hiqdev/composer-config-plugin": "^1.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "common": "config/common.php",
            "web": "config/web.php",
            "defines": "config/defines.php",
            "params": "config/params.php"
        }
    },
    "autoload": {
        "psr-4": {"app\\": "src"}
    },
    "config": {
        "foxy": {
            "enable-packages": {
                "*": true
            }
        },
        "fxp-asset": {
            "enabled": false
        }
    }
}
